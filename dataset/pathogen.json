{
  "$schema": "https://raw.githubusercontent.com/nextstrain/nextclade/refs/heads/release/packages/nextclade-schemas/input-pathogen-json.schema.json",
  "schemaVersion": "3.0.0",
  "files": {
    "reference": "reference.fasta",
    "pathogenJson": "pathogen.json",
    "genomeAnnotation": "genome_annotation.gff3",
    "treeJson": "tree.json",
    "examples": "sequences.fasta",
    "readme": "README.md",
    "changelog": "CHANGELOG.md"
  },
  "attributes": {
    "name": "Enterovirus D68",
    "reference name": "Human enterovirus 68 strain Fermon, complete genome.",
    "reference accession": "AY426531.1"
  },
  "alignmentParams": {
    "penalityGapExtend":3,
    "penaltyGapOpen": 15,
    "penaltyGapOpenInFrame": 20,
    "penaltyGapOpenOutOfFrame": 25,
    "kmerLength": 6,
    "kmerDistance": 15,
    "minMatchLength": 25,
    "allowedMismatches": 10,
    "minLength": 100,
    "gapAlignmentSide": "right",
    "minSeedCover": 0.40
  },
  "defaultCds": "VP1",
  "compatibility": {
        "cli": "3.0.0",
        "web": "3.0.0"
    },
  "mutLabels": {
  "nucMutLabelMap": {
    }
  },
  "qc": {
    "frameShifts": {
      "enabled": true,
      "scoreWeight": 100
    },
    "missingData": {
      "enabled": true,
      "missingDataThreshold": 200,
      "scoreBias": 2400
    },
    "mixedSites": {
      "enabled": true,
      "mixedSitesThreshold": 10
    },
    "privateMutations": {
      "enabled": true,
      "cutoff": 120,
      "typical": 50,
      "weightLabeledSubstitutions": 1.5,
      "weightReversionSubstitutions": 2,
      "weightUnlabeledSubstitutions": 1
    },
    "divergence": {
      "enabled": true,
      "maxDivergence": 0.15,
      "scoreWeight": 50
    },
    "stopCodons": {
      "enabled": true,
      "scoreWeight": 40
    },
    "recombinants": {
      "enabled": true,
      "mutationsThreshold": 140,
      "scoreWeight": 50
    }
    },
    "cdsOrderPreference": [
      "VP4",
      "VP2",
      "VP3",
      "VP1",
      "2A",
      "2B",
      "2C",
      "3A",
      "3B",
      "3C",
      "3D"
    ]
  }