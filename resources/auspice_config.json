{
  "title": "Enterovirus D68 - Nextclade dataset tree",
  "build_url": "https://github.com/hodcroftlab/nextclade_d68",
  "maintainers": [
    {
      "name": "Nadia Neuner-Jehle",
      "url": "https://www.swisstph.ch/en/staff/profile/people/nadia-neuner-jehle"
    },
    {
      "name": "Emma Hodcroft",
      "url": "http://emmahodcroft.com/"
    }
  ],  "extensions": {
    "nextclade": {
      "ref_nodes": {
        "default": "__root__",
        "builtins": {
          "__root__": {
            "displayName": "Reference [AY426531.1]",
            "description": "Fermon strain"
          }
        },
        "search": [
          {
            "name": "Static ancestral root",
            "displayName": "Static ancestral root",
            "description": "Inferred ancestral root [not changing]",
            "criteria": [
              {
                "node": [
                  {
                    "name": ["ancestral_sequence"]
                  }
                ]
              }
            ]
          },
          {
            "name": "Inferred root",
            "displayName": "Tree root",
            "description": "Inferred root by Nextclade",
            "criteria": [
              {
                "node": [
                  {
                    "name": ["NODE_0000000"]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  },  "colorings": [
    {
      "title": "Clade",
      "type": "categorical",
      "key": "clade_membership"
    },
    {
      "title": "Date",
      "type": "ordinal",
      "key": "date"
    },
    {
      "title": "Country",
      "type": "categorical",
      "key": "country"
    },
    {
      "title": "Global region",
      "type": "categorical",
      "key": "region"
    },
    {
      "title": "Epitope at BC loop (Pos 90,92,95,97,98,103)",
      "type": "categorical",
      "key": "BC"
    },
    {
      "title": "Epitope at DE loop (Pos 140,141,142,143,144,145,146,149)",
      "type": "categorical",
      "key": "DE"
    },
    {
      "title": "Epitope at C terminus",
      "type": "categorical",
      "key": "CTERM"
    }
    
  ],
  "geo_resolutions": [
    "country", "region"
  ],
  "filters": [
    "country",
    "region",
    "clade_membership"
  ],
  "display_defaults": {
    "color_by": "clade_membership",
    "geo_resolution": "country",
    "tip_label": "strain",
    "map_triplicate": true
  },
  "metadata_columns": [
    "strain"
  ],
  "panels": [
    "tree",
    "map",
    "entropy"
  ]
}
