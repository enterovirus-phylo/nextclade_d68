{
  "title": "Enterovirus D68 - Nextclade dataset tree",
  "build_url": "https://github.com/hodcroftlab/nextclade_d68",
  "maintainers": [
    {
      "name": "Nadia Neuner-Jehle",
      "url": "https://eve-lab.org/people/nadia-neuner-jehle/"
    },
    {
      "name": "Emma Hodcroft",
      "url": "https://eve-lab.org/people/emma-hodcroft/"
    }
  ],  
  "extensions": {
    "nextclade": {
      "clade_node_attrs": [
      ],
      "ref_nodes": {
        "default": "__root__",
        "builtins": {
          "__root__": {
            "displayName": "Reference [AY426531.1]",
            "description": "Fermon strain"
          }
        },
        "search": [
          {
            "name": "InferredAncestor",
            "displayName": "Static Inferred Ancestor",
            "description": "Precomputed ancestral sequence (static)",
            "criteria": [
              {
                "node": [
                  {
                    "name": ["ancestral_sequence"]
                  }
                ]
              }
            ]
          },
          {
            "name": "Inferred root",
            "displayName": "Tree root",
            "description": "Inferred ancestral root (regenerated on each run)",
            "criteria": [
              {
                "node": [
                  {
                    "name": ["NODE_0000000"]
                  }
                ]
              }
            ]
          }
        ]
      }
    }
  },  "colorings": [
    {
      "title": "Clade",
      "type": "categorical",
      "key": "clade_membership"
    },
    {
      "title": "Date",
      "type": "ordinal",
      "key": "date"
    },
    {
      "title": "Country",
      "type": "categorical",
      "key": "country"
    },
    {
      "title": "Global region",
      "type": "categorical",
      "key": "region"
    },
    {
      "title": "Epitope at BC loop (Pos 90,92,95,97,98,103)",
      "type": "categorical",
      "key": "BC"
    },
    {
      "title": "Epitope at DE loop (Pos 140-148)",
      "type": "categorical",
      "key": "DE"
    },
    {
      "title": "Epitope at C terminus",
      "type": "categorical",
      "key": "CTERM"
    }
    
  ],
  "geo_resolutions": [
    "country", "region"
  ],
  "filters": [
    "new_node",
    "clade_membership",
    "country",
    "region"
  ],
  "display_defaults": {
    "color_by": "clade_membership",
    "distance_measure": "div",
    "tip_label": "strain",
    "branch_label": "clade",
    "map_triplicate": false,
    "transmission_lines": false
  },
  "metadata_columns": [
    "strain"
  ],
  "panels": ["tree","entropy"]
}
